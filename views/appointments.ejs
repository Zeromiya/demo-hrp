<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">
    <meta name="author" content="">

    <title>Demo HRP</title>
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.0.12/css/all.css" integrity="sha384-G0fIWCsCzJIMAVNQPfjH08cyYaUtMwjJwqiRKxxE/rx96Uroj1BtIQ6MLJuheaO9" crossorigin="anonymous">
    <!-- Bootstrap core CSS -->
    <link href="css/bootstrap.min.css" rel="stylesheet">

    <!-- Custom styles for this template -->
    <link href="css/main.css" rel="stylesheet">
</head>

<body>
<nav class="navbar navbar-expand-lg navbar-dark bg-primary">
  <a class="navbar-brand" href="#">HRP</a>
  <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarText" aria-controls="navbarText" aria-expanded="false" aria-label="Toggle navigation">
    <span class="navbar-toggler-icon"></span>
  </button>
  <div class="collapse navbar-collapse" id="navbarText">
    <ul class="navbar-nav mr-auto">
      <li class="nav-item">
        <a class="nav-link" href="/">Home <span class="sr-only">(current)</span></a>
      </li>
      <li class="nav-item active">
        <a class="nav-link" href="/appointment">My Appointments</a>
      </li>
      <li class="nav-item">
      <a class="nav-link" href="/appointment/get">Get Appointment</a>
    </li>
      <li class="nav-item">
        <a class="nav-link" href="/faq">FAQ</a>
      </li>
    </ul>
    <span class="navbar-text">
        <a class="p-1 text-light" >Welcome Mr.Test</a>
        <a class="p-2 text-light" href="/faq">Logout</a>
    </span>
  </div>
</nav>
<div class="card-body">
    <h2 class="display-4">My Appointments</h2>
    <table class="table">
        <thead>
             <tr>
                <% for (var k = 1; k < Object.keys(resulty.metaData).length; k++) { %>
                    <th>
                    <%= resulty.metaData[k].name %>
                    </th>
                <% } %>
                <th>
                </th>
            </tr>
        </thead>
        <tbody>
            <% for (var i = 0; i < resulty.rows.length; i++) { %>
                <tr>
                    <% for (var k = 1,enable = true; k < resulty.rows[i].length; k++, enable=true) { %>
                        <td>
                        <% if(!resulty.metaData[k].name.includes("DIST") && !resulty.metaData[k].name.includes("STATUS")){ %>
                            <a id="<%= resulty.metaData[k].name %><%= resulty.rows[i][0] %>"type="text" class="form-control" ><%= resulty.rows[i][k] %></a>
                        <% }else{ %>
                            <a id="<%= resulty.metaData[k].name %><%= resulty.rows[i][0]%>" type="text" class="form-control" ><%= resulty.rows[i][k] %></a>
                        <% } %>

                        </td>
                    <% } %>
                    <td>
                        <a name="butts" id = "<%= resulty.rows[i][0] %>" class="btn btn-primary" href="/appointment/view/<%= resulty.rows[i][0] %>">View Appointment</a>
                    </td>
                </tr>
                <% } %>
        </tbody>
    </table>
</div>
</div>




<!-- Bootstrap core JavaScript
================================================== -->
